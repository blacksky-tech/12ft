{
    "manifest_version": 3,
    "name": "Zeig mol!",
    "description": "Prepends 'https://12ft.io/' to the current tab URL and navigates to it",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Click 'Zeig mol!'"
    },
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "background.js"
    }
}
